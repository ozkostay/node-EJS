<%- include('./port/layout-start', {title: title}) %>

<main class="main-conteiner">
  <% store.map ((el, idx) => { %>
  <table class="table-items">
    <tr>
      <td style="width: 20px"><%=idx + 1%>.</td>
      <td style="width: 400px" class="el-title"><%=el.id%></td>
      <td style="width: 40px"><button class="edit">Редактировать</button></td>
      <td style="width: 40px">
        <form method="POST" action="api/books/delete/<%=el.id%>">
          <button class="delete">Удалить</button>
        </form>
      </td>
    </tr>
  </table>
  <% }); %>
</main>

<%- include('./port/layout-end') %>
